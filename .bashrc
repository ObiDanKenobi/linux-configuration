###   ALIASES   ###

#navigation
alias ..='cd ..'
alias ...='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'

#ls
alias ls='ls -l'
alias ll='ls -l'
alias lsa='ls -la'

#apt
alias install='sudo apt install'
alias update='sudo apt update'
alias upgrade='sudo apt install'
